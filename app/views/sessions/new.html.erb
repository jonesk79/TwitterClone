<%= content_for(:title, "Login | Twitter Clone") %>

<h1>Log In</h1>

<%= form_tag sessions_path do %>
  <div class="form-group">
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>
  <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <%= submit_tag "Log In"%>
<% end %><br>

<%= link_to "Home", users_path, :class => "btn btn-primary" %>
